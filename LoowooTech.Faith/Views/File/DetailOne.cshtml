
@{
    ViewBag.Title = "原始文件查看";
    Layout = "~/Views/Shared/_Layout.cshtml";
    FaithFile file = ViewBag.Model;
}

<div class="page-header">
    <h3 class="text-center">@(file.Name)</h3>
</div>

<div class="row">
    <div class="col-md-12 col-xs-12 col-sm-12">
        <img src="/@(file.Path)" alt="@(file.Name)" class="img-rounded img-responsive" />
    </div>
</div>

