@{ 
    Layout = null;
    List<Enterprise> list = ViewBag.List;
    EnterpriseParameter parameter = ViewBag.Parameter;
    var page = parameter.Page;

}

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/Site.css" rel="stylesheet" />
